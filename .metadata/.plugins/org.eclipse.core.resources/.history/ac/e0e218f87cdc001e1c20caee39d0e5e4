package unidad6.tarea5.galeria;

/**
 * 
 * @author RUTH CONDE ALEMÁN
 * 
 *         La clase Artista tendrá estos 3 atributos: nombre, web y telefono.
 *         Esta clase Artista implementará los métodos get/set y el método
 *         toString.
 * 
 *         - La clase Artista tendrá 1 constructor, el cual recibirá por
 *         parámetro 2 campos: nombre y teléfono.
 */

public class Artista {

	private String nombre;
	private String web;
	private int telefono;

	public Artista(String nombre, int telefono) {

		this.nombre = nombre;
		this.telefono = telefono;
	}

	public String getNombre() {
		return nombre;
	}

	public void setNombre(String nombre) {
		this.nombre = nombre;
	}

	public String getWeb() {
		return web;
	}

	public void setWeb(String web) {
		this.web = web;
	}

	public int getTelefono() {
		return telefono;
	}

	public void setTelefono(int telefono) {
		this.telefono = telefono;
	}

	@Override
	public String toString() {
		return "Artista [nombre=" + nombre + ", web=" + web + ", telefono=" + telefono + "]";
	}

	

}
